    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.28.4",
    "@vitest/ui": "^0.28.4",
    "@vue/test-utils": "^2.2.10",
    "bumpp": "^8.2.1",
    "eslint": "^8.34.0",
    "happy-dom": "^8.2.6",
    "typescript": "^4.9.5",
    "unplugin-auto-import-api": "^0.0.1",
    "unplugin-vue-components": "^0.23.0",
